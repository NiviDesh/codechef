#include <iostream>
#include<vector>
using namespace std;

int main() {
	// your code goes here
	int test;
	cin>>test;
	int matches[] = {6,2,5,5,4,5,6,3,7,6};
	while(test--){
		int a, b ,n=0;
		vector<int>v;
		cin>>a>>b;
		int sum = a+b;
		while(sum !=0){
		    int rem = sum %10;
		    n = n+matches[rem]; 
		    sum = sum /10;
		}
		
		        
		
		cout<<n<<endl;
		n=0;
	}
	
	
	return 0;
}
