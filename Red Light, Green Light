#include <iostream>
using namespace std;

int main() {
    int testcase,no_people,height, count=0;
    cin>>testcase;
    while(testcase--){
    cin>>no_people>>height;
    int arr[no_people];
    for(int i = 0 ; i<no_people ;i++){
        cin>>arr[i];
    }
    for(int i =0 ; i<no_people; i++){
        if(arr[i]>height){
            count +=1;
        }
    }
    cout<<count<<endl;  
    }
	// your code goes here
	return 0;
}
